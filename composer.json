{
    "name": "grafite/scaffold",
    "type": "project",
    "description": "An opinionated Laravel boilerplate.",
    "keywords": [
        "boilerplate",
        "laravel",
        "grafite"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravelcollective/auth"
        }
    ],
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "grafite/charts": "^0.5.0",
        "grafite/database": "^2.3",
        "grafite/forms": "^4.0",
        "intervention/image": "^2.5",
        "laravel/cashier": "^12.3",
        "laravel/framework": "^8.6",
        "laravel/legacy-factories": "^1.0",
        "laravel/helpers": "^1.3",
        "laravelcollective/auth": "dev-master",
        "lasserafn/php-initial-avatar-generator": "^4.0",
        "spatie/laravel-honeypot": "^2.3",
        "tightenco/ziggy": "^0.9.4"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "itsgoingd/clockwork": "^4.1",
        "laravel/dusk": "^6.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/ActivityHelper.php",
            "app/Helpers/NotificationHelper.php",
            "app/Helpers/UserInterfaceHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check-style": "php-cs-fixer fix --dry-run",
        "fix-style": "php-cs-fixer fix"
    }
}
