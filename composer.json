{
    "name": "grafite/scaffold",
    "type": "project",
    "description": "An opinionated Laravel boilerplate.",
    "keywords": [
        "boilerplate",
        "laravel",
        "grafite"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.192",
        "barryvdh/laravel-debugbar": "^3.6",
        "bepsvpt/secure-headers": "^7.2",
        "grafite/charts": "^2.0",
        "grafite/database": "^2.5.2",
        "grafite/forms": "^6.2",
        "grafite/html": "^1.0",
        "grafite/maintenance": "^0.3.0",
        "grafite/mission-control-laravel": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "laravel/cashier": "^13.0",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.1",
        "laravel/sanctum": "^3.0",
        "laravelcollective/auth": "^1.0",
        "lasserafn/php-initial-avatar-generator": "^4.0",
        "livewire/livewire": "^2.2",
        "pragmarx/google2fa-laravel": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "shiftonelabs/laravel-sqs-fifo-queue": "^2.0",
        "silviolleite/laravelpwa": "^2.0",
        "spatie/laravel-honeypot": "^4.0",
        "tightenco/ziggy": "^1.0.4"
    },
    "require-dev": {
        "brianium/paratest": "^7.0",
        "codedungeon/phpunit-result-printer": "^0.31.0",
        "fakerphp/faker": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/dusk": "^7.0",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/ActivityHelper.php",
            "app/Helpers/NotificationHelper.php",
            "app/Helpers/RouteHelper.php",
            "app/Helpers/SessionHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check-style": "vendor/bin/php-cs-fixer fix --dry-run",
        "fix-style": "vendor/bin/php-cs-fixer fix",
        "analyse": "vendor/bin/phpstan analyse",
        "insights": "vendor/bin/phpinsights",
        "test": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --coverage-clover clover.xml && php coverage-checker.php clover.xml 50",
        "test-debug": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --coverage-html test-html"
    }
}
